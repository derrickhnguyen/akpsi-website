doctype html
html(lang='en')
	head

		title= 'CSULB Alpha Kappa Psi'

		meta(name='google-site-verification', content='YUh0zVrRezkqZlm-dvcYp-IEkzdZOrLyO5ysRlIinv4')
		meta(name='description', content='The Official Website of Alpha Kappa Psi Delta Omicron Chapter of California State University, Long Beach')
		meta(name='keywords', content='CSULB, AKPsi, Delta, Omicron, Chapter')
		meta(name='author', content='Derrick Nguyen')
		meta(charset='utf-8')

		meta(name='viewport', content='width=device-width, initial-scale=1')

		link(href='https://fonts.googleapis.com/css?family=Raleway:300', rel='stylesheet', type='text/css')
		link(href='https://fonts.googleapis.com/css?family=Montserrat', rel='stylesheet', type='text/css')
		link(href='https://fonts.googleapis.com/css?family=Open+Sans:700,400,800', rel='stylesheet', type='text/css')

		link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css')
		link(rel='stylesheet', href='http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css')
		link(rel='stylesheet', type='text/css', href='/assets/stylesheets/min/vendor.css')

	body(class='montserrat')
		noscript(style='color:red') 
			div.container(class='text-center') **Please enable JavaScript to view full effect of website! 

		div.container-fluid(class='parallax-window home', data-parallax='scroll', data-image-src=parallaxUrl, data-position=backgroundPosition, style={'height':height})
			nav.navbar.navbar-transparent
				div.container-fluid
					div.navbar-header
						button(type='button', class='navbar-toggle', data-toggle='collapse', data-target='#nav')
							span.icon-bar
							span.icon-bar
							span.icon-bar
						a.navbar-brand(href='/') CSULB AKPSI
					div#nav.collapse.navbar-collapse
						ul.nav.navbar-nav.navbar-right
							li.active
								a(href='/') HOME
							li.active
								a(href='/about') ABOUT
							li.active
								a(href='/brothers') BROTHERS
							li.active
								a(href='/contact') CONTACT
							li.active
								a(href='/recruitment') RECRUITMENT
			div.title(class='text-center open-sans')
				h1 #{header}
				h2 #{subheader}
				a(href='/about')
					button.btn.btn-default(type='button') ENTER
	include ./includes/scripts.pug
