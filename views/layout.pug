doctype html
html(lang='en')
	head

		title= 'CSULB Alpha Kappa Psi'

		meta(name='google-site-verification', content='YUh0zVrRezkqZlm-dvcYp-IEkzdZOrLyO5ysRlIinv4')
		meta(name='description', content='The Official Website of Alpha Kappa Psi Delta Omicron Chapter of California State University, Long Beach')
		meta(name='keywords', content='CSULB, AKPsi, Delta, Omicron, Chapter')
		meta(name='author', content='Derrick Nguyen')
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1')
			
		include ./includes/styles

	body(class='montserrat')
		noscript(style='color:red') 
			div.container(class='text-center') **Please enable JavaScript to view full effect of website! 

		div.container-fluid(class='parallax-window home', data-parallax='scroll', data-image-src=parallaxUrl, data-position=backgroundPosition, data-natural-width='2000', data-natural-height='1200', style={'height':height})
			nav.navbar
				div.container-fluid
					div.navbar-header
						button(type='button', class='navbar-toggle', data-toggle='collapse', data-target='#nav')
							span.icon-bar
							span.icon-bar
							span.icon-bar
						a.navbar-brand(href='/') CSULB AKPSI
					div#nav.collapse.navbar-collapse
						ul.nav.navbar-nav.navbar-right
							li.active
								a(href='/') HOME
							li.active
								a(href='/about') ABOUT
							li.active.dropdown
								a.dropdown-toggle(href='#', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') BROTHERS
									span(class='caret')
								ul.dropdown-menu
									li.active
										a(href='/brothers') Active Brothers
									li.active
										a(href='/brothers/biographies') Biographies
							li.active
								a(href='/recruitment') RECRUITMENT
							li.active
								a(href='/contact') CONTACT
			div.title(class='text-center open-sans')
				h1 #{header}
				h2 #{subheader}

		block content